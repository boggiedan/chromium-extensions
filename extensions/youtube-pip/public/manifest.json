      "16": "icon16.png",
      "19": "icon19.png",
      "32": "icon32.png",
      "38": "icon38.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon16.png",
        "19": "icon19.png",
        "32": "icon32.png",
        "38": "icon38.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  